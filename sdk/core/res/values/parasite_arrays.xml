<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- The list of vendor package signatures that should also be considered
         platform signatures, specifically for use on devices with a vendor partition. -->
    <string-array name="config_vendorPlatformSignatures" />

    <!-- The list of Google platform package signatures
         that should also be considered platform signatures. -->
    <string-array name="config_googlePlatformSignatures" />

    <!-- The list of components which should be automatically disabled for a specific device.
         Note: this MUST not be used to randomly disable components, ask for approval first! -->
    <string-array name="config_deviceDisabledComponents" translatable="false" />

    <!-- The list of components which should be automatically disabled for all devices. -->
    <string-array name="config_globallyDisabledComponents" translatable="false" />

    <!-- The list of components which should be forced to be enabled. -->
    <string-array name="config_forceEnabledComponents" translatable="false" />

    <!-- Define device configs on boot -->
    <string-array name="global_device_configs_override" translatable="false" />
    <string-array name="device_configs_override" translatable="false" />

    <!-- Define phenotype/gservices flags -->
    <string-array name="global_phenotype_flags_override" translatable="false"/>
    <string-array name="device_phenotype_flags_override" translatable="false"/>
    <!-- Additional namespaces for packages' phenotype/gservices flags -->
    <string-array name="global_phenotype_package_namespaces" translatable="false"/>
    <string-array name="device_phenotype_package_namespaces" translatable="false"/>

    <!-- Prop imitation hook -->
    <string-array name="config_piHookPropsPixelXL" translatable="false"/>
    <string-array name="config_piHookPropsPixelLegacy" translatable="false"/>
    <string-array name="config_piHookProcessPixelLegacy" translatable="false"/>
    <string-array name="config_piHookPropsPixelTablet" translatable="false"/>
    <string-array name="config_piHookPropsPixelExtra" translatable="false"/>
    <string-array name="config_piHookProcessPixelExtra" translatable="false"/>
    <string-array name="config_piHookPropsPixelRecent" translatable="false"/>
    <string-array name="config_piHookProcessPixelRecent" translatable="false"/>
    <string-array name="config_piHookProcessKeep" translatable="false"/>

    <!-- Custom features hook -->
    <string-array name="config_cfHookFeaturesPixel" translatable="false"/>
    <string-array name="config_cfHookFeaturesPixelOthers" translatable="false"/>
    <string-array name="config_cfHookFeaturesPixelTensor" translatable="false"/>
    <string-array name="config_cfHookPackagesFeaturesPixelTensor" translatable="false"/>
    <string-array name="config_cfHookFeaturesNexus" translatable="false"/>
    <string-array name="config_cfHookFeaturesAndroid" translatable="false"/>

    <!-- Whitelist of package names to bypass foreground service type validation -->
    <string-array name="config_fgsTypeValidationBypassPackages" translatable="false"/>
</resources>
