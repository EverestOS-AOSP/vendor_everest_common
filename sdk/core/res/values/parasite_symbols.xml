<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- The list of vendor package signatures that should also be considered
         platform signatures, specifically for use on devices with a vendor partition. -->
    <java-symbol type="array" name="config_vendorPlatformSignatures" />

    <!-- The list of Google platform package signatures
         that should also be considered platform signatures. -->
    <java-symbol type="array" name="config_googlePlatformSignatures" />

    <!-- Package Manager -->
    <java-symbol type="array" name="config_deviceDisabledComponents" />
    <java-symbol type="array" name="config_globallyDisabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Define device configs on boot -->
    <java-symbol type="array" name="global_device_configs_override" />
    <java-symbol type="array" name="device_configs_override" />

    <!-- Define phenotype/gservices flags -->
    <java-symbol type="array" name="global_phenotype_flags_override" />
    <java-symbol type="array" name="device_phenotype_flags_override" />
    <!-- Additional namespaces for packages' phenotype/gservices flags -->
    <java-symbol type="array" name="global_phenotype_package_namespaces" />
    <java-symbol type="array" name="device_phenotype_package_namespaces" />

    <!-- Prop imitation hook -->
    <java-symbol type="array" name="config_piHookPropsPixelXL" />
    <java-symbol type="array" name="config_piHookPropsPixelLegacy" />
    <java-symbol type="array" name="config_piHookProcessPixelLegacy" />
    <java-symbol type="array" name="config_piHookPropsPixelTablet" />
    <java-symbol type="array" name="config_piHookPropsPixelRecent" />
    <java-symbol type="array" name="config_piHookProcessPixelRecent" />
    <java-symbol type="array" name="config_piHookProcessKeep" />

    <!-- Custom features hook -->
    <java-symbol type="array" name="config_cfHookFeaturesPixel" />
    <java-symbol type="array" name="config_cfHookFeaturesPixelOthers" />
    <java-symbol type="array" name="config_cfHookFeaturesPixelTensor" />
    <java-symbol type="array" name="config_cfHookPackagesFeaturesPixelTensor" />
    <java-symbol type="array" name="config_cfHookFeaturesNexus" />
    <java-symbol type="array" name="config_cfHookFeaturesAndroid" />

</resources>
